#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/bin/node_modules:/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/node_modules:/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules:/home/uz/r2-drive/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/bin/node_modules:/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/node_modules:/home/uz/r2-drive/node_modules/.pnpm/workerd@1.20250906.0/node_modules:/home/uz/r2-drive/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/bin/workerd" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/workerd@1.20250906.0/node_modules/workerd/bin/workerd" "$@"
fi
