#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules:/home/uz/r2-drive/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/node_modules:/home/uz/r2-drive/node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules:/home/uz/r2-drive/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/@opennextjs+cloudflare@1.8.2_wrangler@4.36.0/node_modules/@opennextjs/cloudflare/dist/cli/index.js" "$@"
fi
